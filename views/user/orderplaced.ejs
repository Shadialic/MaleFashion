    <!--================Order Details Area =================-->
    <section class="order_details section_gap" style="background: rgb(242, 244, 237);">
      
        <div class="container" id="order" style="padding: 25px;max-width: 80%;">

            <div class="row order_d_inner">
                <div class="col-lg-4">
                    <div class="details_item" style="text-decoration: none; color: black;">
                        <h4 style="border-bottom: 1px solid #c0b4b4;
                        padding-bottom: 12px;
                        font-size: 18px;
                        color: #222222;
                        padding-left: 18px;">Order Info </h4>
                        <ul class="list" style=" list-style: none; font-family: ui-monospace;">
                            <li><a href="#" style="color: #7e848b;"><span>Order number</span> : 60235</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>Date</span> : Los Angeles</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>Total</span> : USD 2210</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>Payment method</span> :<%=user.paymentMethod%></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="details_item">

                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="details_item">
                        <h4 style="border-bottom: 1px solid #c0b4b4;
                        padding-bottom: 12px;
                        font-size: 18px;
                        color: #222222;
                        padding-left: 18px;">Billing Address</h4>
                      <ul class="list" style='list-style: none; color: black; font-family: ui-monospace;'>
                            <li><a href="#" style="color: #7e848b;"><span>Street</span> : 56/8</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>City</span> : Malappuram</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>state</span> : kerala</a></li>
                            <li><a href="#" style="color: #7e848b;"><span>pincodecode </span> : 676306</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <br>
            <br>
            <div class="order_details_table" style="background-color: #fff; padding: 17px; ">
                <h2 style="font-family: initial">Order Details</h2>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th scope="col">Product</th>
                                <th scope="col">Quantity</th>
                                <th scope="col">Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% user.product.forEach((product)=> { %>
                                <tr>
                                    <td>
                                        <p>
                                            <%= product.productId.productname %>
                                        </p>
                                    </td>
                                    <td>
                                        <h5><%= product.quantity %>
                                        </h5>
                                    </td>
                                    <td>
                                        <p>Rs:<%=product.productId.price%>
                                        </p>
                                    </td>
                                </tr>
                                <% }) %>
                                    <tr>
                                        <td>
                                            <h4>Subtotal</h4>
                                        </td>
                                        <td>
                                            <h5></h5>
                                        </td>
                                        <td>
                                            <p>Rs:<%= user.totalAmount %>
                                            </p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <h4>Shipping</h4>
                                        </td>
                                        <td>
                                            <h5></h5>
                                        </td>
                                        <td>
                                            <p>Free</p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <h4>Total</h4>
                                        </td>
                                        <td>
                                            <h5></h5>
                                        </td>
                                        <td>
                                            <p>Rs:<%= user.totalAmount %>
                                            </p>
                                        </td>
                                    </tr>
                        </tbody>
                    </table>
                    <h3 class="title_confirmation" style="text-align: center;
                    color: #28d500;
                    font-size: 18px;
                    margin-bottom: 80px;
                ">Thank you. Your order has been received.</h3>
                </div>
            </div>



        </div>
        <div class="">
            <button type="button" onclick="downloadPDF()" class="btn btn-success m-2">download invoice</button>
        </div>
    </section>
    <!--================End Order Details Area =================-->



